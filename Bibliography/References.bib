@article{Ruiz2010,
	abstract = {Las políticas públicas se consideran como el proceso de aprendizaje colectivo entre sociedad y gobierno, donde la comunicación democrática ayuda en la solución de conflictos. Para el recurso agua, los problemas no son recientes pero sí persistentes y han dado lugar a la realización de políticas públicas desde hace más de treinta años tratando de encontrar soluciones para un manejo adecuado de este recurso. Este artículo aborda la evolución del enfoque de las políticas públicas realizadas a nivel internacional, y también se analiza su trascendencia en el nivel nacional y local, contemplando un estudio de caso realizado en el municipio de Zapotitlán de Vadillo, Jalisco, México, del que se expone su situación actual del manejo del agua desde un enfoque de gobernanza.},
	author = {Silvia {Salcido Ruiz} and Peter R. W. Gerritsen and {Luis Manuel} {Martínez R.}},
